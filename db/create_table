
use ojk;
create table spot 
(
    datetime datetime primary key,
    name varchar(32) primary key,
    gu varchar(32),
    min_population int,
    max_population int,
    temp float,
    precipitation float,
    rain_chance float,
    precpt_type varchar(32),
    air_idx float,
    sky_stts varchar(32),
    humidity float,
    wind_spd float


);
create table station
(
    name varchar(32) primary key,
    day varchar(32),
    line varchar(32),
    direction varchar(32),
